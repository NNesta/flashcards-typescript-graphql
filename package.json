  "devDependencies": {
    "@types/bcryptjs": "~2.4.0",
    "@types/jsonwebtoken": "~8.5.0",
    "prisma": "^3.15.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.7.3",
    "nodemon": "^2.0.16"
  },
  "dependencies": {
    "@prisma/client": "^3.15.1",
    "apollo-server": "^3.8.2",
    "bcryptjs": "~2.4.0",
